<div class='dc-col-header' {{#if isDiscrete}}discrete{{/if}}>
  <h4>
    {{label}}
    <button on-click='sort-by-col:{{index}},false'>▲</button>
    <button on-click='sort-by-col:{{index}},true'>▼</button>
    <button on-click='show-scatter-plots:{{index}},'>[SP]</button>
  </h4>
  {{#if isDiscrete}}
    <div class='disc-value'>{{hoverValue}}</div>
  {{else}}
    <!-- <canvas class='dc-col-scatter' width={{width}} height=160></canvas>-->
    <svg class='dc-ch-axis' height='20' width='{{width}}'></svg>
  {{/if}}
</div>
