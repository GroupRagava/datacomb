{
  "name": "datacomb",
  "version": "0.0.1",
  "description": "An interactive tool for exploring large, tabular datasets.",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && mocha test/*-test.js",
    "serve": "./node_modules/serve/bin/serve -p 5050",
