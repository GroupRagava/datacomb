  "bugs": {
    "url": "https://github.com/cmpolis/datacomb/issues"
  },
  "homepage": "https://github.com/cmpolis/datacomb",
  "dependencies": {
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "serve": "^1.4.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "stringify": "^3.1.0"
  }
